        <%         
'SQL = "select distinct instrument_name,description,quote_mid,rate_date from tRates where product_type='currencies' and instrument_name='"&instrument_name&"' and tenor='spot' and datediff(day,rate_date,getDate()) <= 365 and datediff(day,rate_date,getDate()) >= 1 order by rate_date desc"

SQL = "select max(cast(floor(cast(rate_date as float)) as datetime)) as rate_date, max(quote_mid) as quote_mid from tRates where product_type='currencies' and  instrument_name='"&instrument_name&"' and tenor='spot' and datediff(day,rate_date,getDate()) <= 365 and datediff(day,rate_date,getDate()) >= 1  group by cast(floor(cast(rate_date as float)) as datetime)  order by cast(floor(cast(rate_date as float)) as datetime) desc"

	Set Rec = server.createobject("ADODB.Recordset")
	Rec.Open SQL,Conn
	if Rec.EOF then
		response.Write("There are currently no currency prices to display")
	else
%>      
 
  <table border="0" cellpadding="0" cellspacing="0" width="220">

<tr><td width="120" class="borderBottomDeals"><h6>Date</h6></td><td width="100" class="borderBottomDeals" align="right"><h6>Spot</h6></td></tr>
 


<%

		Do while Not Rec.EOF	
		ratedate = formatdatetime(Rec("rate_date"),2)
		if isnull(ratedate) then
			ratedate="&nbsp;"
		end if
		quotemid = formatnumber(Rec("quote_mid"),4)
		if isnull(quotemid) then
			quotemid = "&nbsp;"
		end if
		
		response.Write("<tr><td class=""borderBottomInside"" height=""25"">"&ratedate&"</td><td class=""borderBottomInside"" height=""25"" align=""right"">"&quotemid&"</td></tr>")
				
		
		Rec.MoveNext
		loop

		response.Write("</table>")
	
	end if

Set Rec=Nothing	

%>   
